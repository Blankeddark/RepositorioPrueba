<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">

	<display-name>SistemaReconocimiento</display-name>

	<welcome-file-list>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>

	<servlet>

		<servlet-name>ServletANS</servlet-name>
		<servlet-class>diccionario.interfaz.ServletANS</servlet-class>

	</servlet>
	
	<servlet>
	
	<servlet-name>ServletDiccionario</servlet-name>
	<servlet-class>diccionario.interfaz.ServletDiccionario</servlet-class>
	
	</servlet>
	
	<servlet>
	
	<servlet-name>ServletReconocimiento</servlet-name>
	<servlet-class>diccionario.interfaz.ServletReconocimiento</servlet-class>
	
	</servlet>
	
	<servlet>
	
	<servlet-name>ServletBusqueda</servlet-name>
	<servlet-class>diccionario.interfaz.ServletBúsqueda</servlet-class>
	
	</servlet>
	
	<servlet>
	
	<servlet-name>ServletCarpetas</servlet-name>
	<servlet-class>diccionario.interfaz.ServletCarpetas</servlet-class>
	
	</servlet>
	
	<servlet>
	
	<servlet-name>ServletPractico</servlet-name>
	<servlet-class>diccionario.interfaz.ServletPráctico</servlet-class>
	
	</servlet>
	
	<servlet-mapping>

		<servlet-name>ServletANS</servlet-name>
		<url-pattern>/ANS.html</url-pattern>

	</servlet-mapping>
	
	<servlet-mapping>
	
		<servlet-name>ServletDiccionario</servlet-name>
		<url-pattern>/diccionario.html</url-pattern>
	
	</servlet-mapping>
	
	<servlet-mapping>
	
	<servlet-name>ServletReconocimiento</servlet-name>
	<url-pattern>/reconocimiento.html</url-pattern>
	
	</servlet-mapping>
	
	<servlet-mapping>
	
	<servlet-name>ServletBusqueda</servlet-name>
	<url-pattern>/busqueda.html</url-pattern>
	
	</servlet-mapping>
	
	<servlet-mapping>
	
	<servlet-name>ServletCarpetas</servlet-name>
	<url-pattern>/carpetas.html</url-pattern>
	
	</servlet-mapping>
	
	<servlet-mapping>
	
	<servlet-name>ServletPractico</servlet-name>
	<url-pattern>/practico.html</url-pattern>
	
	</servlet-mapping>
</web-app>